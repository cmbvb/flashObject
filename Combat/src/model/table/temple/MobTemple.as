package model.table.temple{	public class MobTemple	{		// 怪物编号		protected var _number#:1;		// 怪物avatar		protected var _avatar:2;		// 怪物攻击		protected var _attack:1;		// 怪物血量		protected var _hp:1;		// 怪物速度		protected var _speed:1;		// 怪物类型		protected var _type:1;		// 怪物死亡加魔法值		protected var _mp:1;		// 怪物功能类型		protected var _funcType:1;		// 不可中buff		protected var _unbuff:3;		public function MobTemple()		{			super();		}		public function get number#():1 {			return _number#;		}		public function set number#(value:1):void {			_number# = value;		}		public function get avatar():2 {			return _avatar;		}		public function set avatar(value:2):void {			_avatar = value;		}		public function get attack():1 {			return _attack;		}		public function set attack(value:1):void {			_attack = value;		}		public function get hp():1 {			return _hp;		}		public function set hp(value:1):void {			_hp = value;		}		public function get speed():1 {			return _speed;		}		public function set speed(value:1):void {			_speed = value;		}		public function get type():1 {			return _type;		}		public function set type(value:1):void {			_type = value;		}		public function get mp():1 {			return _mp;		}		public function set mp(value:1):void {			_mp = value;		}		public function get funcType():1 {			return _funcType;		}		public function set funcType(value:1):void {			_funcType = value;		}		public function get unbuff():3 {			return _unbuff;		}		public function set unbuff(value:3):void {			_unbuff = value;		}	}}