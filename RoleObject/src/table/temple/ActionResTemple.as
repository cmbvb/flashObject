package table.temple{	public class ActionResTemple	{		// 编号		protected var _id:String;		// 动作		protected var _actionId:int;		// 最大帧数		protected var _maxAnimFrame:int;		// 帧数配置		protected var _animFrames:Array;		// 音频配置		protected var _soundEffects:Array;		public function ActionResTemple()		{			super();		}		public function get id():String {			return _id;		}		public function set id(value:String):void {			_id = value;		}		public function get actionId():int {			return _actionId;		}		public function set actionId(value:int):void {			_actionId = value;		}		public function get maxAnimFrame():int {			return _maxAnimFrame;		}		public function set maxAnimFrame(value:int):void {			_maxAnimFrame = value;		}		public function get animFrames():Array {			return _animFrames;		}		public function set animFrames(value:Array):void {			_animFrames = value;		}		public function get soundEffects():Array {			return _soundEffects;		}		public function set soundEffects(value:Array):void {			_soundEffects = value;		}	}}